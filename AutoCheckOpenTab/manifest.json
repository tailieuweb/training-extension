{
    "manifest_version": 2,
    "name": "Open List Auto Check",
    "description": "Open list auto check",
    "version": "1.0",
    "permissions": ["tabs", "*://*/*", "storage"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["public/js/jquery.js", "public/js/content.js"]
    }],
    "browser_action": {
        "default_title": "Check Auto",
        "default_popup": "popup.html",
        "default_icon": "public/images/vote.png"
    }

}