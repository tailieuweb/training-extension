{
    "manifest_version": 2,
    "name": "Autocheck",
    "description": "Extension điểm danh tự động dành cho sever",
    "version": "1.0",
    "permissions": ["tabs", "*://*/*", "storage"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["public/js/jquery.js", "public/js/content.js"]
    }],
    "browser_action": {
        "default_title": "Autocheck",
        "default_popup": "popup.html",
        "default_icon": "public/images/checked.png"
    }

}