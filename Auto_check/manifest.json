{
    "manifest_version": 2,
    "name": "Auto check",
    "description": "Dùng để cập nhật điểm danh",
    "version": "1.0",
    "permissions": ["tabs", "://*/" , "storage"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["public/js/jquery.js" , "public/js/content.js"]
    }],
    "browser_action": {
        "default_title": "Auto Logged",
        "default_popup": "popup.html",
        "default_icon": "public/image/icon.png"
    }
}