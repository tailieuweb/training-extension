{
    "manifest_version": 2,
    "name": "CheckAuto",
    "description": "checkouto extention",
    "version": "1.0",
    "permissions": ["tabs", "://*/", "storage"],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["/public/js/jquery.js","/public/js/chucnang.js","/public/js/index.js"]
    }],
    "browser_action": {
        "default_title": "CheckAuto",
        "default_popup": "index.html",
        "default_icon": "/public/images/icon.png"
    }
}